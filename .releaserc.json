{
  "plugins": [
    "semantic-release-gitmoji",
    "@semantic-release/github",
    "@semantic-release/npm",
    [
      "@semantic-release/git", {
        "message": ":bookmark: v${nextRelease.version} [skip ci]\n\nhttps://github.com/cowsay-blog/hexo-webpack/releases/tag/${nextRelease.gitTag}"
      }
    ]
  ]
}